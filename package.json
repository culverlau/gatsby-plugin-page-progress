{
  "name": "gatsby-plugin-page-progress",
  "description": "Progress bar for the top of pages as user scrolls",
  "version": "2.0.6-beta.1",
  "author": "Devin Metivier <devinmetivier@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/dmetivier/gatsby-plugin-page-progress",
  "homepage": "https://github.com/dmetivier/gatsby-plugin-page-progress",
  "bugs": {
    "url": "https://github.com/dmetivier/gatsby-plugin-page-progress/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "plugin",
    "progress",
    "scroll",
    "indicator",
    "bar"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . && uglifyjs gatsby-browser.js -cm -o gatsby-browser.js",
    "clean": "rm -rf node_modules",
    "prepare": "npm run build",
    "watch": "babel -w src --out-dir .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "babel-eslint": "^9.0.0",
    "babel-preset-gatsby-package": "0.1.4",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-devjmetivier": "0.0.8",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "prettier": "^1.16.4",
    "uglify-js": "^3.5.15"
  }
}
